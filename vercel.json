{
    "builds": [{
        "src": "hello/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build_files.sh",
  "use": "@vercel/static",
  "config": {
    "distDir": "staticfiles_build"}
    }],


    "routes": [
        {
            "src": "/(.*)",
            "dest": "hello/wsgi.py"
        },

        {

            "src": "/static/(.*)",
            "dest": "staticfiles/$1"
        }


    ]
}