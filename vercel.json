{
    "builds": [{
        "src": "hello/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles" }
    }],


    "routes": [
        {
            "src": "/(.*)",
            "dest": "hello/wsgi.py"
        },

        {

            "src": "/static/(.*)",
            "dest": "staticfiles/$1"
        }


    ]
}